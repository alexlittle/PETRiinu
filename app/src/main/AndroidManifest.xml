<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="riinu.consult.alexlittle.net.petriinu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mikko"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/mikko"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MedicationActivity"
            android:label="Mikko - Medication">
        </activity>

        <activity
            android:name=".MealActivity"
            android:label="Mikko - Meals">
        </activity>

        <activity
            android:name=".ExerciseActivity"
            android:label="Mikko - Exercise/Activity">
        </activity>

        <activity
            android:name=".PhotoActivity"
            android:label="Mikko - Photos">
        </activity>
    </application>

</manifest>